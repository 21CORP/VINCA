<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <FrameLayout
        android:layout_height="88dp"
        android:layout_gravity="center"
        android:layout_width="88dp">

        <ImageView
            android:id="@+id/symbol"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/symbol_max_size"
            android:maxHeight="@dimen/symbol_max_size"
            android:maxWidth="@dimen/symbol_max_size"
            app:srcCompat="@drawable/activity"
            android:adjustViewBounds="true"
            android:layout_marginRight="@dimen/element_outer_margin"
            android:layout_marginEnd="@dimen/element_outer_margin"
            android:layout_marginLeft="@dimen/element_outer_margin"
            android:layout_marginStart="@dimen/element_outer_margin"
            android:layout_gravity="center"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView5"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:textAlignment="center" />

            <ImageButton
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:srcCompat="@android:drawable/ic_menu_edit"
                android:id="@+id/imageButton3"
                android:background="@android:color/transparent"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true" />

        </RelativeLayout>

    </FrameLayout>

</merge>
