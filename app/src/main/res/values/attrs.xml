<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableView">
        <attr name="borderLeft" format="reference"/>
        <attr name="borderRight" format="reference"/>
    </declare-styleable>
    <declare-styleable name="VincaElementView">
        <attr name="symbol" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="description" format="string"/>
        <attr name="type" format="enum">
            <enum name="Project" value="0"/>
            <enum name="Process" value="1"/>
            <enum name="Activity" value="2"/>
            <enum name="Iteration" value="3"/>
            <enum name="Method" value="4"/>
            <enum name="Pause" value="5"/>
            <enum name="Decision" value="6"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ElementView">
        <attr name="nodes" format="integer"/>
    </declare-styleable>
</resources>